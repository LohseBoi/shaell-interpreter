$per = 2 + 1

fn $printHalloShaell($nej)
    $print("Hej fra shæll :)")
    $print($nej)
end

fn $funcMedReturVaerdi($n)
    return 2 + $n
end

fn $callbackfn($n)
    fn $funktionDerPrintern($n)
        $print($n)
    end
    return $funktionDerPrintern
end

fn $medCallback($callback)
    $callback(2)
end

$medCallback($callbackfn(5))

$print("Hællo from shæll!")

$str = "abcdefg"

$i = 0
while $i < $str:length() do
    $print($str[$i])
    $i = $i + 1
end

$print($str[3 + 1])

$printMenBedre = $print

$printMenBedre("Hej fra bedre print")


$printHalloShaell("Ekstra besked")

$printHalloShaell(120)

$print(
    "det er da fed at få retur værdier som ", 
    $funcMedReturVaerdi(4), 
    " og vide at 391 + 2902 = ", 
    391 + 2902, 
    "!"
)

$obj = {thing = 1}

$print("haha ", $obj:thing + 2, " benis")
"skokd":length()

$print("haha benis")

if true then
    $print("haha benis")
end

if false then
    $print("nej ikke benis")
end

$thing = {[0] = 0, [1] = 1, [6] = 6, [3] = 3, [2] = 2, [19] = 19}

$thing:insert("4 som streng")
$thing:insert(5, 7)

$i = 0
while $i < $thing:length() do
    $print($thing[$i])
    $i = $i + 1
end

$print("------------------")

$print($thing:length())

$fib_cache = {[0] = 0, [1] = 1}

fn $fib($n)
    if $fib_cache[$n] == null then
        $fib_cache[$n] = $fib($n - 1) + $fib($n - 2)
    end
    return $fib_cache[$n]
end

$var = 123:sqrt()

$print($var)
$print(23.12:log2())
$print(2313.32:log(24))

$print("------------------")

$obj = {[0] = 0, [1] = 1, [2] = 2}

$debug_break($obj)
$debug_break($T)

$print($T:length($obj))

$T:insert($obj, 3, 4)

$print($T:length($obj))

$print("-" * 10)
$print(9 * "-")

$print("rrgr" + "ksfogk")
$print("skok" + 492)
$print(942 + "bob")

